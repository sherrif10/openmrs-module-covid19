<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE module PUBLIC "-//OpenMRS//DTD OpenMRS Config 1.0//EN"
		"https://resources.openmrs.org/doctype/config-1.6.dtd">
<module configVersion="1.6">
	
	<!-- Base Module Properties -->
	<id>${project.parent.artifactId}</id>
	<name>${project.parent.name}</name>
	<version>${project.parent.version}</version>
	<package>org.openmrs.module.covid19</package>
	<author>OpenMRS</author>
	<description>
			${project.parent.description}
	</description>

	<activator>org.openmrs.module.covid19.COVID19Activator</activator>
	
	<!-- <updateURL>https://modules.openmrs.org/modules/download/@MODULE_ID@/update.rdf</updateURL> -->
	<!-- /Base Module Properties -->
	
	<require_version>${openmrsPlatformVersion}</require_version>

	<require_modules>
	<require_module version="${metadatamappingVersion}">
			org.openmrs.module.metadatamapping
		</require_module>
		<require_module version="${dataexchangeModuleVersion}">
            org.openmrs.module.dataexchange
        </require_module>
         <require_module version="${metadatadeployVersion}">
            org.openmrs.module.metadatadeploy
        </require_module>
		<require_module version="${appframeworkVersion}">
			org.openmrs.module.appframework
		</require_module>
		<require_module version="${uiframeworkVersion}">
			org.openmrs.module.uiframework
		</require_module>
		<require_module version="${uicommonsModuleVersion}">
			org.openmrs.module.uicommons
		</require_module>
		<require_module version="${appuiVersion}">
			org.openmrs.module.appui
		</require_module>
	</require_modules>

	<aware_of_modules>
		<aware_of_module version="${htmlformentryVersion}">org.openmrs.module.htmlformentry</aware_of_module>
		<aware_of_module version="${htmlformentryuiVersion}">org.openmrs.module.htmlformentryui</aware_of_module>
		<aware_of_module version="${legacyuiVersion}}">org.openmrs.module.legacyui</aware_of_module>
	</aware_of_modules>

	<!-- Internationalization -->
	<!-- All message codes should start with @MODULE_ID@.* -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->
	
</module>

